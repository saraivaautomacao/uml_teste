  object query1: TFDQuery
    Filtered = True
    OnFilterRecord = Query1FilterRecord
    CachedUpdates = True
    Connection = dmMod.fdConPrinc
    SQL.Strings = (
      
        'Select p.codigo,p.produto,p.unidade,p.precovenda,p.quant,p.estfi' +
        'scal,'
      
        'p.cts,p.lklab,p.codigoauxliar,p.lksetor, p.naosaitabela,p.descon' +
        'to,p.sgf_descExtra,'
      'gr.setor,lab.laboratorio from tabEst1  p'
      'left join tabest8 gr'
      'on p.lksetor=gr.controle'
      'left join tablab  lab'
      'on p.lklab=lab.controle'
      'Where'
      'p.naosaitabela='#39'N'#39' and p.codigo<>'#39'99999'#39'  and p.codigo<>'#39'99998'#39)
    Left = 264
    Top = 256
